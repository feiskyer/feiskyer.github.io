probe kernel.function("*@net/socket.c").call{       
    printf("%s -> %s\n", thread_indent(1), ppfunc())
}
probe kernel.function("*@net/socket.c").inline {
    printf("%s -- %s\n", thread_indent(0), ppfunc())
}
probe kernel.function("*@net/socket.c").return {
    printf("%s <- %s\n", thread_indent(-1), ppfunc())
}
